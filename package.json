{
  "name": "swag-labs",
  "version": "1.0.0",
  "description": "Proyecto de automatizaci√≥n E2E con Cypress para Swag Labs",
  "main": "index.js",
  "scripts": {
    "clean:reports": "rimraf cypress/reports",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run cypress run --browser chrome --headless",
    "report:merge": "mochawesome-merge cypress/reports/mochawesome/mochawesome_*.json --output cypress/reports/mochawesome/mochawesome.json",
    "report:generate": "marge cypress/reports/mochawesome/mochawesome.json --reportDir cypress/reports --reportFilename report.html --inline",
    "report:full": "npm run clean:reports && npm run cypress:run && npm run report:merge && npm run report:generate"
  },
  "author": "Sebastian",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^15.1.0",
    "cypress-multi-reporters": "^2.0.5",
    "mocha-junit-reporter": "^2.2.1",
    "mocha-multi-reporters": "^1.5.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.2.0",
    "rimraf": "^6.0.1"
  }
}
